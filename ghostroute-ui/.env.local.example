# RPC Providers (at least one required for Sepolia)
NEXT_PUBLIC_INFURA_API_KEY=your_infura_key
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_key

# WalletConnect (required for RainbowKit)
# Get a free projectId at https://cloud.walletconnect.com/sign-up
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_project_id

# Contract Addresses (deployed on Sepolia)
NEXT_PUBLIC_PRIVACY_VAULT_ADDRESS=0x0000000000000000000000000000000000000000

# Feature Flags
NEXT_PUBLIC_ENABLE_ZK_PROVING=true
NEXT_PUBLIC_NETWORK=sepolia
